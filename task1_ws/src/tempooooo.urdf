<!-- Gazebo Plugins -->

  <!-- Gazebo plugin for skid steering / diff drive control -->
<gazebo>
  <plugin name="husky_diff_drive_controller" filename="libgazebo_ros_diff_drive.so">
    <ros><namespace>/husky_robot_model</namespace></ros>
    <updateRate>100</updateRate>

    <left_joint>husky_robot_model__front_left_wheel_joint</left_joint>
    <right_joint>husky_robot_model__rear_right_wheel_joint</right_joint>

    <wheel_separation>0.5708</wheel_separation>
    <wheel_diameter>0.3302</wheel_diameter>
    <torque>30</torque>

    <commandTopic>cmd_vel</commandTopic>
    <odometryTopic>odom</odometryTopic>
    <odometryFrame>husky_robot_model/odom</odometryFrame>
    <robotBaseFrame>husky_robot_model__base_link</robotBaseFrame>
  </plugin>
</gazebo>

  

